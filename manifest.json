{
  "manifest_version": 2,
  "name": "AdShield",
  "description": "A simple ad blocker for Chrome browser",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": { "default_popup": "popup.html" },
  "background": { "scripts": ["background.js"] },
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"]
}
